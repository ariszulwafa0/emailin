{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["sin1"],
  "env": {
    "NEXTAUTH_URL": "@nextauth_url",
    "DATABASE_URL": "@database_url"
  },
  "build": {
    "env": {
      "NEXTAUTH_URL": "@nextauth_url"
    }
  },
  "functions": {
    "app/api/email/send/route.ts": {
      "maxDuration": 30
    },
    "app/api/cloud/route.ts": {
      "maxDuration": 30
    }
  }
}